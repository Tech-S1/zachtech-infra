terraform {
  required_providers {
    cloudflare = {
      source  = "cloudflare/cloudflare"
      version = "~> 4.0"
    }
    aws = {
      source  = "hashicorp/aws"
      version = "~> 5.0"
    }
  }
}

provider "cloudflare" {
}

provider "aws" {
  region = "eu-west-2"
}

provider "aws" {
  alias = "us-east-1"

  region = "us-east-1"
}
